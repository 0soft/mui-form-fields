{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": 100
    },
    "interface-name": false,
    "no-var-requires": false,
    "quotemark": [true, "single", "jsx-double"],
    "trailing-comma": true,
    "member-access": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [100]
    }
  }
}
